{
    "name": "tex-textlint",
    "version": "1.0.0",
    "description": "TeX auto calibration using textlint + prh",
    "main": "index.js",
    "scripts": {
        "textlint-check": "ls /src/*.tex | grep -v 'main' | xargs $(npm bin)/textlint",
        "textlint-fix": "ls /src/*.tex | grep -v 'main' | xargs $(npm bin)/textlint --fix"
    },
    "author": "MaineK00n",
    "dependencies": {
        "textlint": "^11.4.0"
    },
    "devDependencies": {
        "prh": "^5.4.4",
        "textlint-filter-rule-comments": "^1.2.2",
        "textlint-filter-rule-whitelist": "^2.0.0",
        "textlint-plugin-latex2e": "^0.2.2",
        "textlint-rule-ja-no-mixed-period": "^2.1.0",
        "textlint-rule-ja-no-redundant-expression": "^2.0.0",
        "textlint-rule-max-ten": "^2.0.3",
        "textlint-rule-no-mix-dearu-desumasu": "^3.0.3",
        "textlint-rule-no-nfd": "^1.0.1",
        "textlint-rule-preset-ja-spacing": "^2.0.1",
        "textlint-rule-preset-ja-technical-writing": "^3.1.0",
        "textlint-rule-prh": "^5.2.0",
        "textlint-rule-spellcheck-tech-word": "^5.0.0"
    }
}
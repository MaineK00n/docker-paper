{
    "name": "tex-textlint",
    "version": "1.0.0",
    "description": "TeX auto calibration using textlint + prh",
    "main": "index.js",
    "scripts": {
        "textlint-check": "ls /src/*.tex | grep -v 'main' | xargs $(npm bin)/textlint",
        "textlint-fix": "ls /src/*.tex | grep -v 'main' | xargs $(npm bin)/textlint --fix"
    },
    "author": "MaineK00n",
    "dependencies": {
        "textlint": "^11.4.0"
    },
    "devDependencies": {
        "prh": "^5.4.4",
        "textlint-filter-rule-comments": "^1.2.2",
        "textlint-filter-rule-whitelist": "^2.0.0",
        "textlint-plugin-latex2e": "^0.3.0",
        "textlint-rule-prh": "^5.2.0"
    }
}
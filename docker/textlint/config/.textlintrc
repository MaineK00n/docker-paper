        "latex2e"
    ],
    "rules": {
        "prh": {
            "rulePaths": [
                "./prh.yml"
            ]
        }
    },
    "filters": {
        "comments": true,
        "whitelist": {
            "allow": [
                "/\\$\\$[\\d\\S]*?\\$\\$/m"
            ]
        }
    }
}